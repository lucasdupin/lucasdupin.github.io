header.clients
  box-shadow inset 0 0 0 1000px rgba(0,0,0, 0.2)

section#clients
  width $content-width
  margin 50px auto

  ul
    list-style none
    text-align center
    margin 0
    padding 0
    > li
      display inline-block
      vertical-align top
      width 176px
      height 100px
      text-align center
      margin 30px 10px
      img
        max-width 90%
        max-height 100%

